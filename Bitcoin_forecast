      "source": [
        "import plotly.graph_objects as go\r\n",
        "\r\n",
        "# Create figure\r\n",
        "fig = go.Figure()\r\n",
        "\r\n",
        "fig.add_trace(\r\n",
        "    go.Candlestick(x=list(df.date),open=list(df.open), high=list(df.high), low=list(df.low), close=list(df.close)))\r\n",
        "# Add range slider\r\n",
        "fig.update_layout(\r\n",
        "    xaxis=dict(\r\n",
        "        rangeselector=dict(\r\n",
        "            buttons=list([\r\n",
        "                dict(count=1,\r\n",
        "                     label=\"1m\",\r\n",
        "                     step=\"month\",\r\n",
        "                     stepmode=\"backward\"),\r\n",
        "                dict(count=6,\r\n",
        "                     label=\"6m\",\r\n",
        "                     step=\"month\",\r\n",
        "                     stepmode=\"backward\"),\r\n",
        "                dict(count=1,\r\n",
        "                     label=\"YTD\",\r\n",
        "                     step=\"year\",\r\n",
        "                     stepmode=\"todate\"),\r\n",
        "                dict(count=1,\r\n",
        "                     label=\"1y\",\r\n",
        "                     step=\"year\",\r\n",
        "                     stepmode=\"backward\"),\r\n",
        "                dict(step=\"all\")\r\n",
        "            ])\r\n",
        "        ),\r\n",
        "        rangeslider=dict(\r\n",
        "            visible=True\r\n",
        "        ),\r\n",
        "        type=\"date\"\r\n",
        "    )\r\n",
        ")\r\n",
        "fig.update_yaxes(fixedrange=False)\r\n",
        "fig.show()\r\n"
      ],
